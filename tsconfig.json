{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "module": "es2015",
        "stripInternal": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "target": "es5",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2017",
            "dom"
        ],
        "paths": {
            "app/*": [
                "src/app/*"
            ],
            "env/*": [
                "src/environments/*"
            ],
            "app-core": [
                "src/app/core"
            ],
            "app-core/*": [
                "src/app/core/*"
            ]
        }
    }
}
